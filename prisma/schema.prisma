generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channel {
  id        String   @id @unique
  createdAt DateTime @default(now())
  lat       String?
  lon       String?
  optout    Boolean  @default(false)
  locked    Boolean  @default(false)
  emoji     String?
}

model User {
  id        String   @id @unique
  createdAt DateTime @default(now())
  lat       String?
  lon       String?
}
